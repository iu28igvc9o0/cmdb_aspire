<<<<<readMetricAlertListByAlertkeyAndLevel>>>>>
{
    "size": 10, 
    "query":{
        "bool":{
            "must":[
            	{
                    "match":{
                        "alert_key":"${alert_key}"
                    }
                },
                {
				    "range":{
				        "alert_level":{
				            "gte": ${alert_level}
				        }
				    }
				},
				{
				    "match":{
				        "moni_result": ${moni_result}
				    }
				},
                {
                    "match":{
                        "history_flag": ${history_flag}
                    }
                }
            ]
        }
    },
    "sort": [
       {
         "alert_level": {
         "order": "asc"
        }
      }
    ]
}
