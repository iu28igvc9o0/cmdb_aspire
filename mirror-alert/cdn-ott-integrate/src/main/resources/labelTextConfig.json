<<<<<readWexinAlertListFromSyncMark>>>>>
{
  "size": 500, 	
  "query": {
    "bool": {
      "must": [
        {
          "range": {
            "scan_mark_time": {
              "gt": ${syncMarkMill},
        	  "lte": ${syncMarkMillEnd}
            }
          }
        },
        {
          "match": {
            "business_source": "OTT"
          }
        }
      ]
    }
  },
  "sort": [
    {
      "scan_mark_time": {
        "order": "asc"
      }
    }
  ]
}
