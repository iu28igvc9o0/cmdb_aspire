<<<<<template_fetch_5xx_percent_rawalert>>>>>
	{
	  "size": 0, 
	  "query": {
	    "bool": {
	      "must": [
	      		{"range": {"@timestamp": {"gte": ${timestamp_gte}, "lt": ${timestamp_lt}}}}
	      ]
	    }
	  },
	  "aggs":{
	        
	        "composite_group":{
	            "composite":{
	                "sources":[
	                	{"province_name_group":{"terms":{"field":"province"}}},
	                    {"req_domain_group":{"terms":{"field":"host"}}},
	                    {"manufacture_group":{"terms":{"field":"manufactor"}}}
	                ],
	                "size":500
	            },
	            "aggs":{
	                "sum_5xx_count":{
	                    "sum":{
	                        "missing":0,
	                        "field":"5xx_count"
	                    }
	                },
	                "total_req_count":{
	                    "sum":{
	                        "missing":0,
	                        "field":"tscount"
	                    }
	                },
	                "last_log_time":{
	                    "max":{
	                        "field":"@timestamp"
	                    }
	                },
	                "percent_5xx":{
	                    "bucket_script":{
	                        "buckets_path":{
	                            "_sum_5xx_count":"sum_5xx_count",
	                            "_total_req_count":"total_req_count"
	                        },
	                        "script":"params._total_req_count == 0 ? 0 : params._sum_5xx_count/params._total_req_count"
	                    }
	                }
	            }
	        }
	    }
	}
